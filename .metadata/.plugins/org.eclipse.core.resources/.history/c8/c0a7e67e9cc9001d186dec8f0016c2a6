#include "Tile.h"
#include "Actor.h"
#include "Background.h"
#include "../api.h"

struct Tile tiles[15];

// Constructor
void GameTile_init() {
	for(short i; i < 5; i++) {
		Tile_init(&tiles[i], i);
	}
}

int main()
{
	configureScaler();
    configureVdma();
    int color = 0;


    while(1)
    {
    	setColorByPaletteId(0, color);
    	color += 1;
    	sleep(1);
    }

    return 0;
}
