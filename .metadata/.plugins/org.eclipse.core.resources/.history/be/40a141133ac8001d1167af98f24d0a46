#include "Background.h"
#include <stdio.h>

// Constructor
void Background_init(Background *b) {
	// Initialize all data elements to 0
	for (int i = 0; i < 64; i++) {
		 for (int j = 0; j < 64; j++) {
		      b->backgroundData[i][j] = 0;
		 }
	}
}

// Setter function to set a specific data element
void Background_setTile(Background *b, int x, int y, short tileId);

// Getter function to get a specific data element
short Background_getTile(Background *b, int x, int y);
